<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="ConnectorResources.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>ConnectorResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="InputServicesRecources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>InputServicesRecources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="ConnectorResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>ConnectorResources.resx</DependentUpon>
      </Compile>
      <Compile Update="InputServicesRecources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>InputServicesRecources.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
